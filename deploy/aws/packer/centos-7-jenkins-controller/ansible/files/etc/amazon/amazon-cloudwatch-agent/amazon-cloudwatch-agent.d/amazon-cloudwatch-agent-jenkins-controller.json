{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/jenkins/jenkins.log",
            "log_group_name": "jenkins-controller",
            "log_stream_name": "{local_hostname}",
            "timestamp_format": "%Y-%m-%dT%H:%M:%S%z"
          }
        ]
      }
    }
  }
}
